    "@ckeditor/ckeditor5-paragraph": "^35.3.0",
    "@ckeditor/ckeditor5-react": "^5.0.2",
    "@rsuite/icons": "^1.0.2",
    "axios": "^1.1.3",
    "bootstrap": "^5.2.2",
    "clsx": "^1.1.1",
    "currency-formatter": "^1.5.9",
    "html-react-parser": "^3.0.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "next": "^12.3.4",
    "next-seo": "^5.15.0",
    "query-string": "^8.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "react-number-format": "^5.1.1",
    "react-slick": "^0.29.0",
