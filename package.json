{
  "private": true,
  "author": "Truyen Mai",
  "type": "module",
  "scripts": {
    "dev": "concurrently  \"npm run dev-sv \" \"npm run dev-next\" ",
    "start": "concurrently  \"npm run next-start\" \"\"npm run sv",
    "dev-sv": "nodemon --experimental-specifier-resolution=node --trace-deprecation app.js",
    "sv": "node --experimental-specifier-resolution=node --trace-deprecation app.js",
    "dev-next": "next dev --port=3002 ",
    "dev-start": "node --experimental-specifier-resolution=node app.js",
    "build:prod": "cross-env --max-old-space-size=500 next build",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8000 next build",
    "next-start": "next start --port=3004"
  },
  "imports": {
    "#server/*": "./server/*",
    "#controller/*": "./server/controller/*",
    "#middleware": "./server/middleware/index.js",
    "#route/*": "./server/route/*",
    "#model": "./server/model/index.js",
    "#constant/*": "./server/constant/*",
    "#uploads/*": "./uploads/*",
    "#common/*": "./server/common/*"
  },
  "dependencies": {
